{
  "name": "deric-accounts",
  "version": "1.1.3",
  "author": "Tomas Barton",
  "summary": "Module for managing user accounts and groups.",
  "license": "Apache-2.0",
  "source": "https://github.com/deric/puppet-accounts",
  "project_page": "https://github.com/deric/puppet-accounts",
